<% content_for :body_content_right do %>
<ul id='gallery'>
	<% @page.images.each do |image| %>
	<li>
		<div class='img' >
			<div class='mask'></div>
			<%= link_to image_tag(image.thumbnail("200x200#c").url), image.thumbnail("900x600").url %>
		</div>
	</li>
	<% end %>
</ul>
<% end %>
<%= render :partial => "/refinery/content_page" %>